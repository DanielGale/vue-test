{
  "name": "vuewebpackexample",
  "version": "1.0.0",
  "description": "Vuejs Example with Webpack",
  "main": "public/index.html",
  "scripts": {
    "start": "webpack-dev-server --inline --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "devBuild": "webpack --config webpack.dev.js",
    "dev": "webpack --hide-modules --watch --config webpack.dev.js",
    "prod": "webpack -p --hide-modules --config webpack.prod.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "vue": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-plugin-copy": "^1.0.1"
  },
  "dependencies": {
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
